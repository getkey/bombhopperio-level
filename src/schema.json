{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/product.schema.json",
	"title": "Level",
	"description": "A BombHopper.io level",
	"type": "object",
	"properties": {
		"name": {
			"type": "string",
			"description": "The level's name",
			"minLength": 1
		},
		"timings": {
			"type": "array",
			"items": [
				{
					"type": "integer",
					"description": "2 stars",
					"minimum": 0
				},
				{
					"type": "integer",
					"description": "3 stars",
					"minimum": 0
				}
			],
			"minItems": 2,
			"additionalItems": false
		}
	}
}
